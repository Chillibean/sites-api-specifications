{
  "description": "A subject",
  "type": "object",
  "properties": {
    "active": {
      "type": "boolean",
      "description": "Indicates if the subject is active",
      "example": true
    },
    "any_study_events_completed": {
      "type": "boolean",
      "description": "Indicates if any study events have been completed for the subject",
      "example": true
    },
    "any_study_events_exist": {
      "type": "boolean",
      "description": "Indicates if any study events exist for the subject",
      "example": true
    },
    "cohort": {
      "type": "string",
      "description": "The study cohort of the subject",
      "example": "Cohort 1",
      "nullable": true
    },
    "contains_incomplete_flagged_tasks": {
      "type": "boolean",
      "description": "Indicates if the subject contains flagged tasks",
      "example": false
    },
    "created_at": {
      "type": "string",
      "description": "Date time for when the subject was created",
      "example": "2024-01-02T11:06:48.566Z"
    },
    "created_by": {
      "type": "string",
      "description": "Full name of subject creator",
      "example": "John Doe"
    },
    "id": {
      "type": "number",
      "description": "The subject id",
      "example": 1
    },
    "number": {
      "type": "string",
      "description": "The serial number of the subject",
      "example": "E001004"
    },
    "study_arm": {
      "oneOf": [
        {
          "$ref": "./StudyArm.json"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "required": [
    "active",
    "any_study_events_completed",
    "any_study_events_exist",
    "contains_incomplete_flagged_tasks",
    "created_at",
    "created_by",
    "id",
    "number"
  ],
  "additionalProperties": false
}
