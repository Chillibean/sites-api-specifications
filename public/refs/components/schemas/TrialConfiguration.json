{
  "description": "A trial configuration",
  "type": "object",
  "properties": {
    "id": {
      "type": "number",
      "description": "The id of trial configuration",
      "example": 1
    },
    "name": {
      "type": "string",
      "description": "The name of trial configuration",
      "example": "Alpha Trial Configuration"
    },
    "published": {
      "type": "boolean",
      "description": "Indicates if the trial configuration is published",
      "example": true
    },
    "study_event_display_name": {
      "type": "object",
      "description": "The study event display name",
      "additionalProperties": {
        "$ref": "./TrialConfigurationObjectName.json"
      }
    },
    "study_procedure_display_name": {
      "type": "object",
      "description": "The study procedure display name",
      "additionalProperties": {
        "$ref": "./TrialConfigurationObjectName.json"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "name",
    "study_event_display_name",
    "study_procedure_display_name"
  ]
}